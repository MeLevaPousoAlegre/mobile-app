{
  "name": "AstroApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "android": "react-native run-android",
    "ios": "cd ios && open phaser.xcworkspace",
    "reverse-ports": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:3000 tcp:3000",
    "warning-clean-up": "echo '\\033[1;33m Please, uninstall the app from your phone or destroy the emulator\n\\033[0m'",
    "reinstall-npm-modules": "npm cache clean && rm -rf node_modules && npm install",
    "gradle-clean": "cd android && ./gradlew clean",
    "ios-clean": "cd ios && rm -rf build",
    "clear-watchman": "watchman watch-del-all",
    "hyper-clean": "npm run warning-clean-up && npm run clear-watchman && npm run ios-clean && rm -rf $TMPDIR/react-* && npm run reinstall-npm-modules && npm run gradle-clean && rnpm link",
    "test": "BABEL_ENV=test mocha src/**/tests/*.js --require react-native-mock/mock.js --compilers js:babel-core/register",
    "test-watch": "npm run test -- --watch --watch-extensions js --growl",
    "shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "test:jest": "jest"
  },
  "dependencies": {
    "color": "^0.11.3",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "react": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-komposer": "^1.13.1",
    "react-native": "^0.32.0",
    "react-native-drawer": "^2.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.2.0",
    "react-native-loading-spinner-overlay": "^0.3.0",
    "react-native-maps": "^0.8.0",
    "react-native-meteor": "^1.0.0-rc9",
    "react-native-navbar": "^1.5.0",
    "react-native-router-flux": "^3.35.0",
    "react-native-simple-warn": "^2.0.0",
    "react-native-spinkit": "^0.1.3",
    "react-native-vector-icons": "^2.1.0",
    "redux": "^3.5.2",
    "rn-meteor-containerize": "^1.0.1"
  },
  "devDependencies": {
    "appium": "^1.5.3",
    "babel-jest": "^15.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^1.9.0",
    "babel-root-import": "^4.1.1",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "jest": "^15.1.1",
    "jest-react-native": "^15.0.0",
    "mocha": "^3.0.2",
    "proxyquire": "^1.7.10",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-native-mock": "^0.2.6",
    "react-test-renderer": "^15.3.1",
    "rnpm-plugin-upgrade": "^0.26.0",
    "sinon": "^1.17.5",
    "wd": "^0.4.0"
  },
  "jest": {
    "preset": "jest-react-native"
  }
}
